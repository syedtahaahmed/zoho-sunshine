    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "jest --runInBand",
        "test:e2e": "start-server-and-test start:e2e 8282 'cypress run --browser chrome'",
        "test:e2e:ci": "start-server-and-test start:e2e 8282 test:e2e:record",
        "test:e2e:open": "cypress open",
        "test:e2e:record": "cypress run --browser chrome --record --config video=true",
        "build": "npm run build:web && npm run build:npm",
        "build:web": "webpack --config webpack/webpack-web.config.js",
        "build:npm": "webpack --config webpack/webpack-host-npm.config.js",
        "start": "node dev-server/server-production",
        "start:e2e": "node dev-server/server-dev-e2e",
        "lint": "eslint . --ext=js,jsx",
        "prettier": "prettier --write .",
        "prettier:lint": "prettier --check .",
        "dev": "nodemon --watch dev-server/ --watch dev-site/index.html --watch dev-site/embedded.html --watch dev-site/cdn.html --watch config/config.json --watch config/default/config.json dev-server/server-development",
        "dev:e2e": "nodemon --watch dev-server/ --watch dev-site/e2e.html --watch config/config.json dev-server/server-dev-e2e",
        "uglify:loader": "node ./tasks/generateLoader.js"
    },
    "main": "lib/index.js",
    "files": [
        "lib/index.js"
    ],
    "homepage": "https://smooch.io",
