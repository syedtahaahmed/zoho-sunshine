{
    "name": "smooch",
    "description": "Smooch.io powered web messaging",
    "analyze": false,
    "version": "5.3.9",
    "keywords": [
        "smooch",
        "javascript",
        "sdk"
    ],
    "author": {
        "name": "Smooch",
        "email": "hello@smooch.io",
        "url": "https://smooch.io"
    },
    "contributors": [
        "Andrew Lavers <andrew@smooch.io>",
